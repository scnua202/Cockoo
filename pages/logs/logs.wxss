.container{
	padding-bottom: 50px;
	max-height: 92%;
}
.log {
	color: #999;
	margin: 15px 10px;
}
.log_item{
	padding: 15px 5px;
}
.log_item text {
	padding-right: 20px;
}
.clear{
	position: absolute;
	bottom: 0px;
	width: 100%;
  text-align: center;
  font-weight: bolder;
}
.clear .clear_btn {
	margin: 10px;
	height: 35px;
	line-height: 35px;
	color:#fff;
	background-color: rgba(255,0,0,0.5);
	border-radius: 98px;
  width: 80%;
  font-weight: bolder;
}
